{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/emerging-text-block",
	"version": "0.1.0",
	"title": "Emerging Text Block",
	"category": "custom-gutenberg-blocks",
	"icon": "arrow-up-alt",
	"description": "Emerging Text Block",
	"supports": {
		"html": false
	},
	"attributes": {
		"animationDuration": {
			"type": "string",
			"default": "500"
		},
		"animationDelay": {
			"type": "string",
			"default": "100"
		},
		"textHeight": {
			"type": "string",
			"default": "50"
		}
	},
	"textdomain": "gutenpride",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"script": "file:./script.js",
	"viewScript": "file:./frontend.js",
	"render": "file:./render.php"
}