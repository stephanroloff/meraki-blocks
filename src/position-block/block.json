{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/position-block",
	"version": "0.1.0",
	"title": "Position Block",
	"category": "widgets",
	"icon": "smiley",
	"description": "Position Block",
	"supports": {
		"html": false
	},
	"attributes": {
		"position": {
			"type": "string",
			"default": "relative"
		},
		"top": {
			"type": "string",
			"default": "auto"
		},
		"bottom": {
			"type": "string",
			"default": "auto"
		},
		"left": {
			"type": "string",
			"default": "auto"
		},
		"right": {
			"type": "string",
			"default": "auto"
		},
		"zindex": {
			"type": "string",
			"default": "0"
		},
		"width": {
			"type": "string",
			"default": "auto"
		},
		"unit": {
			"type": "string",
			"default": ""
		}
	},
	"textdomain": "gutenpride",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"script": "file:./script.js",
	"viewScript": "file:./frontend.js",
	"render": "file:./render.php"
}