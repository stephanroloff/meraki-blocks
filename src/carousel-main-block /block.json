{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/carousel-main-block",
	"version": "0.1.0",
	"title": "Carousel Main Block",
	"category": "carl-bosch-blocks",
	"description": "Carousel Main Block",
	"supports": {
		"html": false,
		"align": true
	},
	"attributes": {
		"autoplay": {
			"type": "boolean",
			"default": false
		},
		"vertical": {
			"type": "boolean",
			"default": false
		},
		"showBullets": {
			"type": "boolean",
			"default": false
		},
		"showArrows": {
			"type": "boolean",
			"default": false
		},
		"delay": {
			"type": "string",
			"default": "2000"
		},
		"speed": {
			"type": "string",
			"default": "1000"
		},
		"editModeActive": {
			"type": "boolean",
			"default": false
		},
		"height": {
			"type": "string",
			"default": "300"
		},
		"effect": {
			"type": "string",
			"default": "slice"
		}
	},
	"textdomain": "gutenpride",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"script": "file:./script.js",
	"viewScript": "file:./frontend.js",
	"render": "file:./render.php"
}