{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/masonry-block",
	"version": "0.1.0",
	"title": "Masonry Block",
	"category": "merakistarter",
	"description": "Masonry Block",
	"supports": {
		"html": false,
		"align": true
	},
	"attributes": {
		"amountSelected": {
			"type": "string",
			"default": "3"
		},
		"overlayHover": {
			"type": "boolean",
			"default": false
		},
		"imageMinWidth": {
			"type": "string",
			"default": "300"
		}
	},
	"textdomain": "gutenpride",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"script": "file:./script.js",
	"viewScript": "file:./frontend.js",
	"render": "file:./render.php"
}