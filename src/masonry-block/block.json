{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/masonry-block",
	"version": "0.1.0",
	"title": "Masonry Block",
	"category": "custom-gutenberg-blocks",
	"icon": "smiley",
	"description": "Masonry Block",
	"supports": {
		"html": false
	},
	"attributes": {
		"renderType": {
			"type": "string",
			"default": "static"
		},
		"postType": {
			"type": "string",
			"default": "post"
		},
		"columns": {
			"type": "string",
			"default": "3"
		},
		"breakpointSmall": {
			"type": "string",
			"default": "640"
		},
		"breakpointMedium": {
			"type": "string",
			"default": "1024"
		},
		"breakpointLarge": {
			"type": "string",
			"default": "1440"
		},
		"breakpointExtraLarge": {
			"type": "string",
			"default": "1920"
		}
	},
	"textdomain": "gutenpride",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"script": "file:./script.js",
	"viewScript": "file:./frontend.js",
	"render": "file:./render.php"
}