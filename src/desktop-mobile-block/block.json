{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/desktop-mobile-block",
	"version": "0.1.0",
	"title": "Desktop Mobile Block",
	"category": "widgets",
	"icon": "smiley",
	"description": "2 Blocks in one, create layouts for Desktop and for Mobile.",
	"supports": {
		"html": false
	},
	"attributes": {
		"layoutType": {
			"type": "string",
			"default": "desktop-view"
		},
		"breakpointWidth": {
			"type": "string",
			"default": "600"
		}
		
	},
	"textdomain": "gutenpride",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"script": "file:./script.js",
	"viewScript": "file:./frontend.js",
	"render": "file:./render.php"
}