{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/text-fill-block",
	"version": "0.1.0",
	"title": "Text-Fill-block",
	"category": "merakistarter",
	"description": "This block contains 2 Headings, the first one is in front and the second one is behind. The headings must have different font colors.",
	"supports": {
		"html": false,
		"align": true
	},
	"icon": "editor-textcolor",
	"attributes": {
		"colorSelectedBackground": {
			"type": "string",
			"default": "#DDDDDD"
		},
		"colorSelectedForeground": {
			"type": "string",
			"default": "#000000"
		},
		"text": {
			"type": "string",
			"default": "Text Placeholder"
		},
		"headingSelected": {
			"type": "string",
			"default": "h2"
		},
		"textAlign":{
			"type": "string",
			"default": "left"
		},
		"browserHeight": {
			"type": "string",
			"default": "7"
		},
		"speed": {
			"type": "string",
			"default": "4"
		}
	},
	"textdomain": "gutenpride",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"script": "file:./script.js",
	"viewScript": "file:./frontend.js",
	"render": "file:./render.php"
}